<nav id="nav" class="show">
    <div id="nav-group">
        <div id="blog_name">
            <% if(theme.nav.left.enable){ %>
                <div class="back-home-button" tabindex="-1">
                    <i class="back-home-button-icon ri-menu-4-line"></i>
                    <%- partial('partial/compoment/nav/left') %>
                </div>
            <% } %>
            <a id="site-name" href="/" title="返回博客主页">
                <span class="heoicon">
                    <%= theme.site.name %>
                </span>
            </a>
        </div>
        <div id="page-name-mask">
            <div id="page-name">
                <% if (page.title) {%>
                <a id="page-name-text" onclick="acrylic.toTop()"><%= page.title %></a>
                <% } else { %>
                <a id="page-name-text" class="blog-title" onclick="acrylic.toTop()"><%= config.title %></a>
                <% } %>
            </div>
        </div>
        <div id="menus">
            <%- partial('partial/compoment/nav/menu') %>
        </div>
        <div id="nav-left">
            <div id="fps-group">
                <div id="fps">145</div><span class="fpsText">FPS</span>
            </div>
        </div>
        <div id="nav-right">
            <%- partial('partial/compoment/nav/right') %>
        </div>
    </div>
</nav>
