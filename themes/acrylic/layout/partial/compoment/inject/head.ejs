<% const cdn = theme.cdn.head %>
<% if(theme.loading.enable && theme.loading.pace){ %>
    <script data-pace-options="{ &quot;restartOnRequestAfter&quot;:false,&quot;eventLag&quot;:false}" src="<%= cdn.pacejs %>"></script>
<% } %>
<%- css(cdn.fontawesomecss) %>
<%- css(cdn.snackbarcss) %>
<%- js(cdn.qrcodejs) %>
<% if(theme.comment.enable){ %>
    <%- js(cdn.twikoojs) %>
<% } %>
<% if(theme.sakana.enable){ %>
    <%- css(cdn.sakanacss) %>
    <script>
      function initSakanaWidget() {
        new SakanaWidget().mount('#sakana-widget');
      }
    </script>
    <script
            async
            onload="initSakanaWidget()"
            src="<%= cdn.sakanajs %>"
    ></script>
<% } %>
<% if(theme.nav.right.search && theme.thirdparty.search.algolia_search.enable){ %>
    <%- js(cdn.instantsearch) %>
    <%- js(cdn.algolia) %>
<% } %>
